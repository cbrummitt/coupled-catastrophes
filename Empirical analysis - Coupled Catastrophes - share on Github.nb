           RowBox[{"VertexList", "[", "graph", "]"}]}], ",", 
          RowBox[{"Except", "[", "None", "]"}]}], "]"}], ",", "1"}], "]"}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Select", "[", 
      RowBox[{"cascadeHopTriplets", ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Avoid", " ", "fan"}], "-", 
         RowBox[{
         "forward", " ", "triplets", " ", "in", " ", "the", " ", 
          "calculation", " ", "of", " ", "cascade"}], "-", 
         RowBox[{"hop", " ", "temporal", " ", "motifs"}]}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Not", "@", 
          RowBox[{"MemberQ", "[", 
           RowBox[{
            RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"\"\<upstream\>\"", "/.", "#"}], ")"}], 
             "\[DirectedEdge]", 
             RowBox[{"(", 
              RowBox[{"\"\<downstream\>\"", "/.", "#"}], ")"}]}]}], "]"}]}], 
         "\[IndentingNewLine]", "&&", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "make", " ", "sure", " ", "that", " ", "the", " ", "downstream", 
           " ", "country", " ", 
           RowBox[{"doesn", "'"}], "t", " ", "have", " ", "links", " ", 
           "pointing", " ", "to", " ", "countries", " ", "that", " ", 
           "protested", " ", "before", " ", "it", " ", "did"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"And", "@@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"First", "[", "#", "]"}], "/.", "eventTimes"}], 
                ")"}], ">", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"Last", "[", "#", "]"}], "/.", "eventTimes"}], 
                ")"}]}], ")"}], "&"}], "/@", 
            RowBox[{"Cases", "[", 
             RowBox[{
              RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
              RowBox[{"_", "\[DirectedEdge]", 
               RowBox[{"(", 
                RowBox[{"\"\<downstream\>\"", "/.", "#"}], ")"}]}]}], "]"}]}],
            ")"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", "&"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
