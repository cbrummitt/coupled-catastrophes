Cell[BoxData[
 RowBox[{
  RowBox[{"findAllHopMotifsReversed", "[", 
   RowBox[{"graph_", ",", "eventTimes_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "cascadeHopTriplets", "}"}], ",", "\[IndentingNewLine]", 
