Cell[BoxData[
 RowBox[{
  RowBox[{"findCascadeHoppingsReversed", "[", 
   RowBox[{"graph_", ",", "upstreamCountry_", ",", "vertexToTimeRules_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "countriesPointingToInitialCountryCatastropheLater", ",", 
      "secondNeighbors", ",", "result"}], "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"countriesPointingToInitialCountryCatastropheLater", "=", 
      RowBox[{"Select", "[", 
       RowBox[{
        RowBox[{"VertexList", "[", "graph", "]"}], 
        RowBox[{"(*", 
         RowBox[{"First", "/@", 
          RowBox[{"Cases", "[", 
           RowBox[{
            RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
            RowBox[{"upstreamCountry", "\[DirectedEdge]", "_"}]}], "]"}]}], 
         "*)"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"MemberQ", "[", 
           RowBox[{
            RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
            RowBox[{"upstreamCountry", "\[DirectedEdge]", "#"}]}], "]"}], "&&",
           "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"#", "/.", "vertexToTimeRules"}], ")"}], ">", 
           RowBox[{"(", 
            RowBox[{"upstreamCountry", "/.", "vertexToTimeRules"}], ")"}]}]}],
          "&"}]}], "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"result", "=", 
      RowBox[{"Reap", "[", "\[IndentingNewLine]", 
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Make", " ", "a", " ", "list", " ", "of", " ", "countries", " ", 
          "pointing", " ", "to", " ", "firstNeighbor", " ", "who", " ", 
          "protested", " ", "earlier", " ", "than", " ", "it", " ", "did", 
          " ", "but", " ", "after", " ", "initial", " ", "country"}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"secondNeighbors", "=", 
           RowBox[{"Select", "[", 
            RowBox[{
             RowBox[{"VertexList", "[", "graph", "]"}], 
             RowBox[{"(*", 
              RowBox[{"Last", "/@", 
               RowBox[{"Cases", "[", 
                RowBox[{
                 RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
                 RowBox[{"firstNeighbor", "\[DirectedEdge]", "_"}]}], "]"}]}],
               "*)"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"MemberQ", "[", 
                RowBox[{
                 RowBox[{"EdgeList", "[", "graph", "]"}], ",", 
                 RowBox[{"firstNeighbor", "\[DirectedEdge]", "#"}]}], "]"}], "&&", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"upstreamCountry", "/.", "vertexToTimeRules"}], 
                  ")"}], "<", 
                 RowBox[{"(", 
                  RowBox[{"#", "/.", "vertexToTimeRules"}], ")"}], "<", 
                 RowBox[{"(", 
                  RowBox[{"firstNeighbor", "/.", "vertexToTimeRules"}], 
                  ")"}]}], ")"}]}], "&"}]}], "]"}]}], ";", 
          "\[IndentingNewLine]", "\[IndentingNewLine]", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"secondNeighbors", "=!=", 
             RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"Do", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"Sow", "[", 
                 RowBox[{"{", 
                  RowBox[{
                   RowBox[{"\"\<upstream\>\"", "\[Rule]", "upstreamCountry"}],
                    ",", 
                   RowBox[{
                   "\"\<intermediate\>\"", "\[Rule]", "firstNeighbor"}], ",", 
                   
                   RowBox[{
                   "\"\<downstream\>\"", "\[Rule]", "secondNeighbor"}]}], 
                  "}"}], 
                 RowBox[{"(*", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{
                    "firstNeighbor", "\[DirectedEdge]", "upstreamCountry"}], 
                    ",", 
                    RowBox[{
                    "secondNeighbor", "\[DirectedEdge]", "firstNeighbor"}]}], 
                   "}"}], "*)"}], "]"}], ";"}], ",", "\[IndentingNewLine]", 
               RowBox[{"{", 
                RowBox[{"secondNeighbor", ",", "secondNeighbors"}], "}"}]}], 
              "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{
          "firstNeighbor", ",", 
           "countriesPointingToInitialCountryCatastropheLater"}], "}"}]}], 
        "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Last", "[", "result", "]"}], "=!=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"result", "\[LeftDoubleBracket]", 
        RowBox[{"2", ",", "1"}], "\[RightDoubleBracket]"}], ",", "None"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input"],

