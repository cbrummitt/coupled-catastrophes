              RowBox[{"Last", "[", "#", "]"}], "]"}], "}"}]}], "]"}], ",", 
          RowBox[{"Last", "[", "#", "]"}]}], "}"}], "]"}]}], "&"}], ")"}], "/@",
     "\[IndentingNewLine]", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"First", "[", "#", "]"}], ",", 
         RowBox[{"Select", "[", 
